.container {
    margin: 20px;
}

.inspector-actor-dropdown-field {
    opacity: 0.6;
    background-color: rgb(89, 71, 68);
    border-width: 2px;
    border-radius: 5px;
    border-color: rgb(55, 55, 55);
    -unity-font-style: bold;
    font-size: 14px;
    min-height: 30px;
    flex-direction: column;
    flex-grow: 1;
}

.inspector-variable-dropdown-field {
