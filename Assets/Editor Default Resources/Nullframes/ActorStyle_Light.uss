.variableBox {
    padding: 10px;
    margin: 10px;
    border-color: #969696;
    border-width: 1px;
}

.infoBox {
    padding: 10px;
    margin: 10px;
    border-color: #969696;
    border-width: 1px;
}

.title-field {
    font-size: 18px;
    -unity-font-style: bold;
    margin-bottom: 5px;
}

.variableLabel {
    font-size: 14px;
    -unity-font-style: bold;
    width: 130px;
    max-width: 130px;
}

.info-label-field {
    font-size: 14px;
}

.remove-variable-button {
    background-color: #874040;
    border-color: #B0B0B0;
    padding: 8px;
    margin: 1px 3px;
    font-size: 12px;
    width: 100px;
    -unity-font-style: bold;
    color: white;
}

.remove-variable-button:hover {
    background-color: #855151;
}

.remove-variable-button:active {
    background-color: #984242;
}

.add-variable-button {
    padding: 8px;
    margin: 1px 3px;
    font-size: 12px;
    width: 100px;
    -unity-font-style: bold;
    border-color: #545353;
    background-color: #ccc5ae;
}

.add-variable-button:hover {
    background-color: #d0ca9d;
}

.add-variable-button:active {
    background-color: #dcd597;
}

.variable-string-text-area >.unity-text-field__input {
    background-color: #ccc5ae;
    border-width: 0;
}

.variable-string-text-area {
    background-color: #ccc5ae;
    border-width: 1px;
    border-color: #545454;
    padding: 8px;
    flex-grow: 1;
    flex-shrink: 1;
    white-space: normal;
    font-size: 12px;
    margin: 0;
    -unity-font-style: bold;
}

.variable-string-text-area:hover {
    border-color: #8d8d8d;
}

.variable-string-text-area:focus {
    border-color: #9b9b9b;
}

